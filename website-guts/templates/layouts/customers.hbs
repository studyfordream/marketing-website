---
layout: wrapper
layout_script: false
layout_body_class: false
TR_subnav:
-
  TR_title: "Customer List"
  link: "customers"
-
  TR_title: "Customer Stories"
  link: "customers/customer-stories"
subnav_first: "customers"
subnav_last: "customers/customer-stories"
---
<div id="main">
  <div id="page-header">
    <div class="container">
      <div class="clearfix page-subheader-cont">
        <h1>{{this.visible_title}}</h1>
        <ul class="sub-header-nav">
          {{#each subnav}}
          <li class="sub-nav">
            <a href="{{../linkPath}}/{{link}}"{{#isCurrentDest ../this.dest link true}} class="active"{{/isCurrentDest}}>{{title}}</a>
          </li>
          {{/each}}
        </ul>
      </div><!---->
    </div><!--/.container-->
  </div><!--/#page-header-->
  <div id="page-header-gradient"></div>
  <div id="page-content">
    {{#unless this.no_container}}<div class="container">{{/unless}}
      {% body %}
    {{#unless this.no_container}}</div><!--/.container-->{{/unless}}
  </div><!--/#content-->
</div><!--/#main-->
